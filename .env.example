# 로그 레벨 설정 (DEBUG, INFO, WARNING, ERROR)
# DEBUG: 모든 로그 출력 (개발/디버깅용)
# INFO: 일반 정보 이상 출력  
# WARNING: 경고 이상만 출력 (기본값, 운영 환경 권장)
# ERROR: 오류만 출력
LOG_LEVEL=WARNING

# 시스템 최적화 설정
PARENT_CHECK_INTERVAL=5               # 부모 프로세스 체크 간격(초) - CPU 최적화
MAX_WEBSOCKET_CONNECTIONS=10          # WebSocket 최대 연결 수 (비정상 접근 차단용)

# 다운로드 관련 설정
MAX_TOTAL_DOWNLOADS=5
DOWNLOAD_PATH=/downloads

# 프록시 설정 (선택사항)
# PROXY_LIST=proxy1:port1,proxy2:port2

# 간단 로그인 처리
AUTH_USERNAME=admin
AUTH_PASSWORD=admin
JWT_SECRET_KEY=your-secret-key